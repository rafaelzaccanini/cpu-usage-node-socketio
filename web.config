<configuration>
  
  <system.web>
    <httpHandlers>
      <add name="iisnode-socketio" path="server-socketio.js" verb="*" modules="iisnode" />  
    </httpHandlers>
  </system.web>
	
  <system.webServer>
    <rewrite>  
      <rules>  
           <rule name="LogFile" patternSyntax="ECMAScript">  
                <match url="socket.io"/>  
                <action type="Rewrite" url="server-socketio.js"/>  
           </rule>  
      </rules>  
 	</rewrite>
	<webSocket enabled="false" />
  </system.webServer>
  
</configuration>


